<%
Collection<Coupon> coupons = (Collection<Coupon>)request.getAttribute("coupons");
out.println("<table border='1'>");
out.println("<tr><th>id</th><th>name</th><th>description</th></tr>");
for(Coupon coupon : coupons) {
	out.println("<tr><td>"+coupon.getId()+"</td><td>"+coupon.getName()+"</td><td>"+coupon.getDescription()+"</td></tr>");	
}
out.println("</table>");
%>

